{
  "name": "os2forms/os2forms_egir",
  "type": "drupal-module",
  "description": "Adds EGIR integration to OS2forms med Forl√∏b",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "GPL-2.0-or-later",
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    "assets": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  },
  "require": {
    "os2forms/os2forms_forloeb": "^1.3.0",
    "drupal/feeds_ex": "^1.0@alpha",
    "drupal/keycloak": "^1.5",
    "zaporylie/composer-drupal-optimizations": "1.2.0",
    "ramsey/uuid": "^4.2"
  },
  "extra": {
    "composer-exit-on-patch-failure": true,
    "patchLevel": {
      "test": "-p2"
    },
    "enable-patching": true,
    "patcher": {
      "force-reset": true
    },
    "patches": {
      "drupal/entity_print": {
        "2733781 - Add Export to Word Support": "https://www.drupal.org/files/issues/2019-11-22/2733781-47.patch"
      },
      "drupal/webform": {
        "Unlock possibility of using Entity print module export to Word": "https://www.drupal.org/files/issues/2020-02-29/3096552-6.patch"
      }
    }
  }
}
